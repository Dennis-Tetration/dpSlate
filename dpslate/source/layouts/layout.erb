<% codePanel_off = (current_page.data.codePanel_off || false) %>
<% if !current_page.data.rightPanel %>
    <%  codePanel_off = true %>
<% else %>
    <% codePanel_off = false %>
<% end %>

<% if !current_page.data.leftPanel  %>
    <%  leftPanel_off = true %>
<% else %>
    <% leftPanel_off = false %>
<% end %>

<% title_page = (current_page.data.title_page && true) %>
<% if !current_page.data.titlePage %>
    <% title_page = false %>
<% else %>
    <% title_page = true %>
<% end %>

<% search = (current_page.data.search && true) %>
<% if !current_page.data.documentSearch %>
    <% search = false %>
<% else %>
    <% search = true %>
<% end %>

<% toc = (current_page.data.toc_enabled && true) %>
<% if !current_page.data.tableOfContents %>
    <%  toc = false %>
<% else %>
    <% toc = true %>
<% end %>

<% toc_accordion = (current_page.data.toc_accordion || false ) %>
<% if current_page.data.tocAccordian %>
    <% toc_accordion = true %>
<% else %>
    <% toc_accordion = false %>
<% end %>

<% language_tabs = current_page.data.language_tabs %>

    <%= stylesheet_link_tag :screen, media: :screen %>
    <%= stylesheet_link_tag :print, media: :print %>
    <% if leftPanel_off %>
      <%= stylesheet_link_tag :inlinetoc_screen, media: :screen %>
      <%= stylesheet_link_tag :inlinecode_screen, media: :screen %>
      <% language_tabs = false %>
    <% end %>
    <% if codePanel_off %>
      <%= stylesheet_link_tag :inlinecode_screen, media: :screen %>
      <% language_tabs = false %>
    <% end %>
    
    <script>
        window.jQuery || document.write('<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"><\/script>')
    </script>
    
    <% if search %>
      <%= javascript_include_tag  "all" %>
    <% else %>
      <%= javascript_include_tag  "all_nosearch" %>
    <% end %>

    <% if toc %>
      <script>
          
        (function (global) {
          'use strict';
          
          var closeToc = function() {
            $(".tocify-wrapper").removeClass('open');
            $("#nav-button").removeClass('open');
          };
        
          var makeToc = function() {
            global.toc = $("#toc").tocify({
              selectors: "<%= current_page.data.toc_selectors || 'h1,h2,h3' %>",
              extendPage: false,
              theme: "none",
              smoothScroll: false,
              showAndHide: <%= toc_accordion %>,
              showEffect: "slideDown",
              showEffectSpeed: 180,
              hideEffect: "slideUp",
              hideEffectSpeed: 180,
              highlightOffset: 60,
              scrollTo: 50,
              scrollHistory: true,
              hashGenerator: function (text, element) {
                return element.prop('id');
              }
            }).data('toc-tocify');
        
            $("#nav-button").click(function() {
              $(".tocify-wrapper").toggleClass('open');
              $("#nav-button").toggleClass('open');
              return false;
            });
        
            $(".page-wrapper").click(closeToc);
            $(".tocify-item").click(closeToc);
          };
        
          // Hack to make already open sections to start opened,
          // instead of displaying an ugly animation
          function animate () {
            setTimeout(function() {
              toc.setOption('showEffectSpeed', 180);
            }, 50);
          }
        
          $(makeToc);
          $(animate);
        })(window);

      </script>
      
      <% if toc_accordion %>
        <style>
          .tocify-subheader {
             display: none;
          }  
        </style>
      <% end %>
      
    <% end %>


    <a href="#" id="nav-button">
      <span>
        NAV
        <%= image_tag('navbar.png') %>
      </span>
    </a>

    <div class="tocify-wrapper">

      <% if toc %>
        <% if title_page %>
          <div class=toc-header>
            <div class=doc-title><%= current_page.data.title %></div>
            <div class="about">
              <div class="about-control"><span>about</span></div>
              <div class="title-page">
                <ul>
                <% if current_page.data.title %>
                  <li><strong><i>Title: </i></strong><%= current_page.data.title %> </li>
                <% end %>
                <% if current_page.data.version %>
                  <li><strong><i>Version: </i></strong><%= current_page.data.version %></li>
                <% end %>
                <% if current_page.data.copyright %>
                  <li><strong><i>Copyright: </i></strong><%= current_page.data.copyright %></li>
                <% end %>
                <% if current_page.data.publisher %>
                  <li><strong><i>Publisher: </i></strong><%= current_page.data.publisher %></li>
                <% end %>
                <% if current_page.data.publisher_address %>
                  <li><strong><i>Publisher Address: </i></strong><%= current_page.data.publisher_address %></li>
                <% end %>
                <% if current_page.data.comments %>
                  <li><strong><i>Comments: </i></strong><%= current_page.data.comments %></li>
                <% end %>
                  <li><strong><i>Published Date: </i></strong><%= datestring %></li>
                </ul>
              </div>
            </div>
          </div>
          <script>      
            $(".about-control").click(function () {
              $(".title-page").slideToggle(500, function () {
                $(".about-control").text(function () {
                  return $(".title-page").is(":visible") ? "hide" : "about";
                });
              });
            });
          </script>
        <% end %>
        <% if search %>
          <div class="search">
            <input type="text" class="search" id="input-search" placeholder="Search Document">
            <ul class="search-results"></ul>
          </div>
        <% end %>
        <div class=toc-header>
          <div class=toc-title>Table of Contents</div>
        </div>
        <div id="toc">
        </div>
        <% if current_page.data.toc_footers %>
          <div class="toc-footer">
            <ul class="toc-footer">
              <li><%= current_page.data.version %></li>
              <% current_page.data.toc_footers.each do |footer| %>
                <li><%= footer %></li>
              <% end %>
              <li>Published <%= datestring %></li> 
            </ul>
          </div>
        <% end %>
      <% end %>
    </div>
    <div class="page-wrapper">
      <div class="dark-box"></div>
      <div class="content">
        <%= yield %>
        <% current_page.data.includes && current_page.data.includes.each do |include| %>
          <%= partial "includes/#{include}" %>
        <% end %>
      </div>
      <div class="dark-box">
        <% if language_tabs %>
          <div class="lang-selector">
            <% language_tabs.each_with_index do |lang, index| %>
              <% if index == 0 %>
                <% classValue = "class='active'" %>
              <% else %>
                <% classValue = "" %>
              <% end %>
              <% if lang.is_a? Hash %>
                <a href="#" data-language-name="<%= lang.keys.first %>" <%= classValue %> ><%= lang.values.first %></a>
              <% else %>
                <a href="#" data-language-name="<%= lang %>" <%= classValue %> ><%= lang %></a>
              <% end %>
            <% end %>
          </div>
        <% end %>
      </div>
    </div>
